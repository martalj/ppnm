set terminal png
set output "spline.png"
set xlabel "x"
set ylabel "y"
set key left
plot "Out.txt" index 0 using 1:2 with points title "tab points",\
"Out.txt" index 1 using 1:2 with lines title "spline",\
"Out.txt" index 1 using 1:3 with lines title "Deriv",\
"Out.txt" index 1 using 1:4 with lines title "Integ",\
"Out.txt" index 1 using 1:5 with lines linetype 2 title "Plain qspline",\
"Out.txt" index 1 using 1:6 with lines linetype 3 title "Plain cspline"

